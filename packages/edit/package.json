    "vite": "^7.0.3",
    "vue-tsc": "^3.0.1"
  },
  "description": "Raw HTML editor component",
  "author": "Studion <info@gostudion.com> (https://github.com/tailor-cms)",
  "dependencies": {
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.9",
